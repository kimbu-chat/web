  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "todomvc"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{html,css,less,ejs,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "module": "esnext",
  "name": "ravudi",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/*.js'",
    "build": "craco build",
    "bundle-analyze": "cross-env REACT_APP_INTERACTIVE_ANALYZE=1 yarn build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "generate-cert": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./certificates.ps1",
    "generate-conf": "sh ./config-generator/generate-config.sh",
    "generate-mac-cert": "sh ./certification/generate-dev-ssl-cert.sh",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "pre-commit": "lint-staged",
    "predeploy": "yarn build",
    "prepare": "husky install",
    "prestart:linux:darwin": "sh ./certification/generate-dev-ssl-cert.sh",
    "prettify": "prettier ./src --write",
    "prettify:check": "prettier ./src --check",
    "start": "yarn generate-conf && run-script-os",
    "start:build": "serve -s build",
    "start:linux:darwin": "craco start",
    "start:win32": "yarn generate-cert && craco start",
    "stylelint": "stylelint ./src",
    "test": "craco test"
  },
  "version": "0.1.0"
}
