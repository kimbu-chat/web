{
	"name": "react-hooks-redux-typescript-boilerplate",
	"version": "1.1.1",
	"private": true,
	"module": "esnext",
	"description": "A frontend boilerplate with React hooks, Redux and Typescript",
	"homepage": "https://rokoroku.github.io/react-redux-typescript-boilerplate",
	"keywords": [
		"react",
		"hooks",
		"redux",
		"typescript",
		"boilerplate",
		"todomvc"
	],
	"main": "index.js",
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "webpack-dev-server --mode development --hot --progress --https",
		"build": "webpack -p --progress --colors",
		"buildAnalyze": "cross-env NODE_ENV='productionAnalyze' webpack --progress --colors",
		"format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
		"lint": "prettier --check . --ext .js,.jsx,.ts,.tsx && eslint . --ext .js,.jsx,.ts,.ts && tsc",
		"lint:fix": "prettier --write . --ext .js,.jsx,.ts,.tsx && eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"analyze": "source-map-explorer 'build/*.js'",
		"prestart": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./certificates.ps1"
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@firebase/app-types": "^0.6.1",
		"@svgr/webpack": "^5.4.0",
		"@types/emoji-mart": "^3.0.2",
		"@types/firebase": "^3.2.1",
		"@types/jwt-decode": "^2.2.1",
		"@types/lodash": "4.14.161",
		"@types/node": "^14.11.1",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/react-image-crop": "^8.1.2",
		"@types/react-infinite-scroller": "^1.2.1",
		"@types/react-redux": "^7.1.9",
		"@types/react-router": "^5.1.8",
		"@types/react-router-dom": "^5.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/webpack": "^4.41.22",
		"@types/webpack-env": "^1.15.3",
		"@types/webrtc": "^0.0.26",
		"babel-loader": "^8.1.0",
		"babel-plugin-import": "^1.13.0",
		"babel-plugin-transform-imports": "^2.0.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.1.1",
		"cross-env": "^7.0.2",
		"css-loader": "^4.3.0",
		"file-loader": "^6.1.0",
		"html-loader": "^1.3.1",
		"html-webpack-plugin": "^4.5.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"mini-css-extract-plugin": "^0.11.2",
		"node-sass": "^4.14.1",
		"npm-check-updates": "^9.0.1",
		"postcss": "^8.0.6",
		"postcss-browser-reporter": "^0.6.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^4.0.2",
		"postcss-preset-env": "^6.7.0",
		"postcss-reporter": "^7.0.0",
		"postcss-url": "^8.0.0",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.0.2",
		"react-hot-loader": "^4.12.21",
		"redux-devtools-extension": "^2.13.8",
		"sass-loader": "^10.0.2",
		"source-map-explorer": "^2.5.0",
		"style-loader": "^1.2.1",
		"ts-loader": "^8.0.4",
		"typescript": "^4.0.3",
		"url-loader": "^4.1.0",
		"webpack": "^4.44.2",
		"webpack-bundle-analyzer": "^3.9.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"@fingerprintjs/fingerprintjs": "^2.1.4",
		"@material-ui/core": "^4.11.0",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@microsoft/signalr": "3.1.8",
		"axios": "^0.20.0",
		"emoji-mart": "^3.0.0",
		"firebase": "^7.21.0",
		"i18next": "^19.7.0",
		"immer": "^7.0.9",
		"jwt-decode": "^2.2.0",
		"libphonenumber-js": "^1.7.58",
		"lodash": "^4.17.20",
		"moment": "^2.28.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-i18next": "^11.7.2",
		"react-image-crop": "^8.6.5",
		"react-infinite-scroller": "^1.2.4",
		"react-redux": "^7.2.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"redux": "^4.0.5",
		"redux-actions": "^2.6.5",
		"redux-saga": "^1.1.3",
		"typesafe-actions": "^5.1.0",
		"use-interval": "^1.3.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"*.{html,css,less,ejs,json}": [
			"prettier --write",
			"git add"
		]
	}
}
