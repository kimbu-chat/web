{
  "name": "ravudi",
  "version": "0.1.0",
  "private": true,
  "module": "esnext",
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "todomvc"
  ],
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@microsoft/signalr": "^5.0.4",
    "@redux-saga/core": "^1.1.3",
    "@sentry/react": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "emoji-mart": "^3.0.1",
    "firebase": "^8.2.10",
    "i18next": "^19.9.2",
    "immer": "^8.0.1",
    "jwt-decode": "^3.1.2",
    "libphonenumber-js": "1.9.14",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mousetrap": "1.6.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-h5-audio-player": "3.5.0",
    "react-i18next": "^11.8.9",
    "react-image-crop": "8.6.6",
    "react-redux": "^7.2.2",
    "react-rnd": "10.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-transition-group": "4.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.1.2",
    "use-interval": "1.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "yarn generate-conf && run-script-os",
    "start:win32": "yarn generate-cert && set HTTPS=true&&react-app-rewired start",
    "start:linux:darwin": "HTTPS=true react-app-rewired start",
    "start:build": "serve -s build",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "generate-cert": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./certificates.ps1",
    "analyze": "source-map-explorer 'build/*.js'",
    "prettify": "prettier ./src --write",
    "prettify:check": "prettier ./src --check",
    "stylelint": "stylelint ./src",
    "generate-conf": "./config-generator/generate-config.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/emoji-mart": "^3.0.4",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/mousetrap": "^1.6.5",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-image-crop": "^8.1.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "run-script-os": "^1.1.6",
    "sass": "^1.32.8",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
