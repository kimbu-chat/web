{
  "name": "react-hooks-redux-typescript-boilerplate",
  "version": "1.1.1",
  "private": true,
  "module": "esnext",
  "description": "A frontend boilerplate with React hooks, Redux and Typescript",
  "homepage": "https://rokoroku.github.io/react-redux-typescript-boilerplate",
  "keywords": [
    "react",
    "hooks",
    "redux",
    "typescript",
    "boilerplate",
    "todomvc"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --mode development --hot --progress --https",
    "build": "cross-env NODE_ENV='production' webpack --progress",
    "buildAnalyze": "cross-env NODE_ENV='productionAnalyze' webpack --progress",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "prettier --check . --ext .js,.jsx,.ts,.tsx && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "prettier --write . --ext .js,.jsx,.ts,.tsx && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "analyze": "source-map-explorer 'build/*.js'",
    "prestart": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./certificates.ps1"
  },
  "license": "MIT",
  "devDependencies": {
